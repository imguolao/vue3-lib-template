{
  "name": "vue3-li-template",
  "version": "0.0.0",
  "description": "... for Vue - It can be used to ... in Vue using the ... library.",
  "author": {
    "name": "guolao",
    "email": "guolao.mq@foxmail.com"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/es/index.js",
  "unpkg": "lib/umd/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "rimraf lib && rollup -c",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md README.*",
    "eslint-fix": "eslint --fix ./src  --ext .vue,.js,.ts,.tsx",
    "preinstall": "npx only-allow pnpm",
    "version": "npm run changelog",
    "prepublishOnly": "npm run build",
    "prepare": "husky install",
    "release": "npx np"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imguolao/vue3-lib-template"
  },
  "keywords": [
    "vue",
    "vue3"
  ],
  "files": [
    "lib",
    "README.zh-CN.md"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imguolao/vue3-lib-template"
  },
  "homepage": "",
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "np": "^7.6.1",
    "only-allow": "^1.1.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.73.0",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-html": "^3.2.0",
    "vue": "^3.2.34"
  },
  "peerDependencies": {
    "vue": "^3.2.34"
  }
}
